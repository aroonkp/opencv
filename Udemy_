<script type="text/javascript">
    // From: From http://www.w3schools.com/js/js_cookies.asp
    function getCookie(cname) {
        var name = cname + "=";
        var ca = document.cookie.split(';');
        for(var i = 0; i <ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0)==' ') {
                c = c.substring(1);
            }
            if (c.indexOf(name) == 0) {
                return c.substring(name.length,c.length);
            }
        }
        return "";
    }

    function getHttpCacheHeaders() {
        var cookies = document.cookie.split(';');
        return cookies
            .filter(function(word) {
                return word.trim().substring(0,9) == 'ud_cache_';
            })
            .map(function(entry) {
                var pairs = entry.split('=');
                var suffix = pairs[0].trim().split('ud_cache_')[1]
                var name = suffix.split('_').map(
                    function(s) {
                        return s.slice(0, 1).toUpperCase() + s.slice(1);
                    }).join('-');
                var key = 'X-Udemy-Cache-'.concat(name)
                return [key, pairs[1]]
            });
    }

    function setHttpCacheHeaders(request) {
        var cacheHeaders = getHttpCacheHeaders()
        for(var i = 0; i < cacheHeaders.length; i++) {
            request.setRequestHeader(cacheHeaders[i][0], cacheHeaders[i][1])
        }
    }

    function makeRequest(method, url) {
        return new Promise(function (resolve, reject) {
            var xhr = new XMLHttpRequest();
            xhr.open(method, url);
            
            var accessToken = getCookie('access_token');
            if (accessToken) {
                var auth = "Bearer " + accessToken;
                xhr.setRequestHeader("Authorization", auth);
                xhr.setRequestHeader("X-Udemy-Authorization", auth);
            }
            setHttpCacheHeaders(xhr);
            xhr.onload = function () {
                if (this.status >= 200 && this.status < 300) {
                    resolve(xhr.response);
                } else {
                    reject({
                        status: this.status,
                        statusText: xhr.statusText
                    });
                }
            };
            xhr.onerror = function () {
                reject({
                    status: this.status,
                    statusText: xhr.statusText
                });
            };
            xhr.send();
        });
    }

    function updateQueryFromRequest(prefetchUrl) {
        if (!window.location.search) {
            return prefetchUrl;
        }

        var firstChar = '?';
        if (prefetchUrl.indexOf('?') >= 0) {
            firstChar = '&';
        }

        return prefetchUrl + firstChar + window.location.search.slice(1);
    }
</script>
